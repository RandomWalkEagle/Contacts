QWidget#SipCallNotifyer
{
	background-color: rgba(0, 0, 0, 220);
}

QLabel
{
	font-size: 11px;
}

QLabel#notice
{
	color: grey;
}

QToolButton
{
	min-height: 16px;
	max-height: 16px;
	height: 16px;
	padding: 0px;
	min-width: 26px;
}

QToolButton:hover
{
	padding: 0px;
}

QToolButton#accept
{
	border-image: url(%IMAGES_PATH%/sipcallbtnok.png);
}

QToolButton#accept:hover
{
	border-image: url(%IMAGES_PATH%/sipcallbtnok_hover.png);
}

QToolButton#accept:pressed
{
	border-image: url(%IMAGES_PATH%/sipcallbtnok_click.png);
}

QToolButton#reject
{
	border-image: url(%IMAGES_PATH%/sipcallbtncancel.png);
}

QToolButton#reject:hover
{
	border-image: url(%IMAGES_PATH%/sipcallbtncancel_hover.png);
}

QToolButton#reject:pressed
{
	border-image: url(%IMAGES_PATH%/sipcallbtncancel_click.png);
}