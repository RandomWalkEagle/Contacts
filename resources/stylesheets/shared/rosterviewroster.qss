/*************
 * QTreeView *
 *************/
QTreeView
{
	background-color: rgb(255, 255, 255);
	alternate-background-color: rgb(239, 239, 239);
}
QTreeView::item
{
	color: rgb(1, 1, 1);
}
QTreeView::item:selected
{
	background-color: rgb(144, 209, 249);
	color: rgb(1, 1, 1);
}
QTreeView::item:hover
{
	background-color: rgba(144, 209, 249, 150);
}
