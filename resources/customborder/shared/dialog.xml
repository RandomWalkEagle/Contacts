<?xml version="1.0" encoding="utf-8"?>
<window-border-style>
 <left-border>
   <width>28</width>
   <resize-width>5</resize-width>
   <resize-margin>28</resize-margin>
   <image src="menuicons/leftShadow" />
 </left-border>
 <top-border>
   <width>28</width>
   <resize-width>5</resize-width>
   <resize-margin>28</resize-margin>
   <image src="menuicons/topShadow" />
 </top-border>
 <right-border>
   <width>28</width>
   <resize-width>5</resize-width>
   <resize-margin>28</resize-margin>
   <image src="menuicons/rightShadow" />
 </right-border>
 <bottom-border>
   <width>28</width>
   <resize-width>5</resize-width>
   <resize-margin>28</resize-margin>
   <image src="menuicons/bottomShadow" />
 </bottom-border>
 <top-left-corner>
   <width>55</width>
   <height>59</height>
   <radius>4</radius>
   <resize-left>28</resize-left>
   <resize-width>10</resize-width>
   <resize-top>28</resize-top>
   <resize-height>10</resize-height>
   <image src="menuicons/topLeftShadow" />
   <mask src="menuicons/topLeftMask" />
 </top-left-corner>
 <top-right-corner>
   <width>57</width>
   <height>61</height>
   <radius>4</radius>
   <resize-right>28</resize-right>
   <resize-width>10</resize-width>
   <resize-top>28</resize-top>
   <resize-height>10</resize-height>
   <image src="menuicons/topRightShadow" />
   <mask src="menuicons/topRightMask" />
 </top-right-corner>
 <bottom-left-corner>
   <width>55</width>
   <height>49</height>
   <radius>4</radius>
   <resize-left>28</resize-left>
   <resize-width>10</resize-width>
   <resize-bottom>28</resize-bottom>
   <resize-height>10</resize-height>
   <image src="menuicons/bottomLeftShadow" />
   <mask src="menuicons/bottomLeftMask" />
 </bottom-left-corner>
 <bottom-right-corner>
   <width>57</width>
   <height>51</height>
   <radius>4</radius>
   <resize-right>28</resize-right>
   <resize-width>10</resize-width>
   <resize-bottom>28</resize-bottom>
   <resize-height>10</resize-height>
   <image src="menuicons/bottomRightShadow" />
   <mask src="menuicons/bottomRightMask" />
 </bottom-right-corner>
 <header>
   <spacing>5</spacing>
   <height>0</height>
   <move-height>26</move-height>
   <margins left="5" right="5" top="7" bottom="2" />
   <gradient type="linear">
     <direction x1="0" y1="0" x2="0" y2="26" />
     <gradient-stop at="0.0" color="#00FF0032" />
     <gradient-stop at="0.5" color="#96FF0096" />
     <gradient-stop at="1.0" color="#00FF0032" />
   </gradient>
 </header>
 <window-icon>
   <width>16</width>
   <height>16</height>
   <icon src="menuicons/mainwindowLogo16" />
 </window-icon>
 <title color="#FF00FF" />
 <window-controls spacing="2" />
 <minimize-button>
   <width>17</width>
   <height>16</height>
   <border width="1" color="#000000" radius="0" image="" />
   <image-normal src="menuicons/minimizeButtonNormal" />
   <image-hover src="menuicons/minimizeButtonHighlighted" />
 </minimize-button>
 <maximize-button>
   <width>17</width>
   <height>16</height>
   <border width="1" color="#000000" radius="0" image="" />
   <image-normal src="menuicons/maximizeButtonNormal" />
   <image-hover src="menuicons/maximizeButtonHighlighted" />
 </maximize-button>
 <close-button>
   <width>17</width>
   <height>16</height>
   <border width="1" color="#000000" radius="0" image="" />
   <image-normal src="menuicons/closeButtonNormal" />
   <image-hover src="menuicons/closeButtonHighlighted" />
 </close-button>
</window-border-style>
