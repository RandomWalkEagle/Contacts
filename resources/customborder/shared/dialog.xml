<?xml version="1.0" encoding="utf-8"?>
<window-border-style>
 <left-border>
   <width>0</width>
   <resize-width>5</resize-width>
   <gradient color="#FF000066" />
 </left-border>
 <top-border>
   <width>0</width>
   <resize-width>5</resize-width>
   <gradient color="#FF000066" />
 </top-border>
 <right-border>
   <width>0</width>
   <resize-width>5</resize-width>
 </right-border>
 <bottom-border>
   <width>0</width>
   <resize-width>5</resize-width>
 </bottom-border>
 <top-left-corner>
   <width>5</width>
   <height>5</height>
   <radius>5</radius>
   <resize-left>0</resize-left>
   <resize-width>10</resize-width>
   <resize-top>0</resize-top>
   <resize-height>10</resize-height>
 </top-left-corner>
 <top-right-corner>
   <width>5</width>
   <height>5</height>
   <radius>5</radius>
   <resize-right>0</resize-right>
   <resize-width>10</resize-width>
   <resize-top>0</resize-top>
   <resize-height>10</resize-height>
 </top-right-corner>
 <bottom-left-corner>
   <width>5</width>
   <height>5</height>
   <radius>5</radius>
   <resize-left>0</resize-left>
   <resize-width>10</resize-width>
   <resize-bottom>0</resize-bottom>
   <resize-height>10</resize-height>
 </bottom-left-corner>
 <bottom-right-corner>
   <width>5</width>
   <height>5</height>
   <radius>5</radius>
   <resize-right>0</resize-right>
   <resize-width>10</resize-width>
   <resize-bottom>0</resize-bottom>
   <resize-height>10</resize-height>
 </bottom-right-corner>
 <header>
   <spacing>5</spacing>
   <height>0</height>
   <move-height>26</move-height>
   <margins left="5" right="5" top="3" bottom="2" />
   <gradient type="linear">
     <direction x1="0" y1="0" x2="0" y2="26" />
     <gradient-stop at="0.0" color="#00FF0032" />
     <gradient-stop at="0.5" color="#96FF0096" />
     <gradient-stop at="1.0" color="#00FF0032" />
   </gradient>
 </header>
 <window-icon>
   <width>16</width>
   <height>16</height>
   <icon src="menuicons/mainwindowLogo16" />
 </window-icon>
 <title color="#FF00FF" />
 <window-controls spacing="2" />
 <minimize-button>
   <width>17</width>
   <height>16</height>
   <border width="1" color="#000000" radius="0" image="" />
   <image-normal src="menuicons/minimizeButtonNormal" />
   <image-hover src="menuicons/minimizeButtonHighlighted" />
 </minimize-button>
 <maximize-button>
   <width>17</width>
   <height>16</height>
   <border width="1" color="#000000" radius="0" image="" />
   <image-normal src="menuicons/maximizeButtonNormal" />
   <image-hover src="menuicons/maximizeButtonHighlighted" />
 </maximize-button>
 <close-button>
   <width>17</width>
   <height>16</height>
   <border width="1" color="#000000" radius="0" image="" />
   <image-normal src="menuicons/closeButtonNormal" />
   <image-hover src="menuicons/closeButtonHighlighted" />
 </close-button>
</window-border-style>

